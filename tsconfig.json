{
  "compilerOptions": {
	"module": "commonjs",
	"target": "es2015",
	"moduleResolution": "Node",
	"strict": true,
	"declaration": true,
	"sourceMap": true,
	"outDir": "./dist",
	"baseUrl": ".",
	"lib": [
	  "es5",
	  "scripthost",
	  "es2015.symbol.wellknown",
	  "es2015.collection"
	]
  },
  "include": [
	"src/**/*"
  ],
  "exclude": [
	"../node_modules",
	"src/**/*.spec.ts"
  ]
}
